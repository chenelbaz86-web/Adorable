generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model App {
  id        String   @id @default(cuid())
  name      String
  createdAt DateTime @default(now())
  users     AppUser[]
}

model AppUser {
  id        String   @id @default(cuid())
  email     String   @unique
  createdAt DateTime @default(now())
  appId     String
  app       App      @relation(fields: [appId], references: [id])
}
